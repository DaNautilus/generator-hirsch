/**
 * <%= capitalizedName %>
 * @namespace <%= moduleName %>
 */
(function() {
    'use strict';

    angular
        .module('<%= moduleName %>.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .filter('<%= capitalizedName %>', <%= capitalizedName %>Filter);

    /**
     * @memberOf <%= moduleName %>
     * @namespace <%= capitalizedName %>
     *
     * @description
     * <%= description %>
     */
    function <%= capitalizedName %>Filter(<%= dependencies %>) {

        return Filter;
        ////////////////////////

        function Filter(input) {
            input = input || '';

            // code goes here

            return input;
        }

    }


}());
