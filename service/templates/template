/**
 * <%= capitalizedName %>
 * @namespace <%= modulePath %>
 */
(function() {
    'use strict';

    angular
        .module('<%= modulePath %>.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .factory('<%= moduleName %><%= capitalizedName %>', <%= moduleName %><%= capitalizedName %>Service);

    /**
     * @memberOf <%= modulePath %>
     * @namespace <%= modulePath %>.<%= lowercaseName %>
     *
     * @description
     * <%= description %>
     */
    function <%= moduleName %><%= capitalizedName %>Service(<%= dependencies %>) {
        var service = {
            key:    'value',

            method: method
        };

        return service;
        ////////////////////////

        /**
         * @name method
         * @desc this is a method who does stuff
         * @memberOf <%= modulePath %>.<%= lowercaseName %>
         */
        function method(){
            // code will be here
        }

    }


}());
