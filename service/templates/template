/**
 * <%= capitalizedName %>
 * @namespace Services
 */
(function() {
    'use strict';

    angular
        .module('common.service.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .factory('<%= prefix %><%= capitalizedName %>', <%= capitalizedName %>Service);

    /**
     * @namespace <%= capitalizedName %>
     * @desc <%= description %>
     * @memberOf Services
     */
    function <%= capitalizedName %>Service(<%= dependencies %>) {
        var service = {
            key:    'value',

            method: method
        };

        return service;
        ////////////////////////

        /**
         * @name method
         * @desc this is a method who does stuff
         * @memberOf Services.<%= capitalizedName %>
         */
        function method(){
            // code will be here
        }

    }


}());