/**
 * <%= capitalizedName %>
 * @namespace <%= moduleName %>
 */
(function() {
    'use strict';

    angular
        .module('<%= moduleName %>.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .factory('<%= capitalizedName %>', <%= capitalizedName %>Service);

    /**
     * @memberOf <%= moduleName %>
     * @namespace <%= capitalizedName %>
     *
     * @description
     * <%= description %>
     */
    function <%= capitalizedName %>Service(<%= dependencies %>) {
        var service = {
            key:    'value',

            method: method
        };

        return service;
        ////////////////////////

        /**
         * @name method
         * @desc this is a method who does stuff
         * @memberOf <%= moduleName %>.<%= capitalizedName %>
         */
        function method(){
            // code will be here
        }

    }


}());
