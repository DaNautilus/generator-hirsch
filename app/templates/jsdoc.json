{
  "tags":      {
    "allowUnknownTags": true,
    "dictionaries":     [
      "jsdoc",
      "closure"
    ]
  },
  "source":    {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "docs"
  },
  "plugins":   [
    "plugins/markdown",
    "node_modules/angular-jsdoc/plugins/ngdoc"
  ],
  "templates": {
    "cleverLinks":           true,
    "monospaceLinks":        true,
    "navType":               "vertical",
    "linenums":              true,
    "collapseSymbols":       true,
    "inverseNav":            true,
    "outputSourceFiles":     true,
    "outputSourcePath":      true,
    "highlightTutorialCode": true,
    "syntaxTheme":           "default",
    "theme":                 "flatly"
  }
}
