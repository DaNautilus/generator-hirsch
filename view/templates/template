/**
 * <%= capitalizedName %>
 * @namespace <%= modulePath %>
 */
(function() {
    'use strict';

    /**
     * @memberOf <%= modulePath %>
     * @namespace <%= lowercaseName %>
     *
     * @description
     * <%= description %>
     * Route: /<%= url %>
     */
    angular
        .module('<%= modulePath %>.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .config(<%= moduleName %><%= capitalizedName %>RouteConfig)
        .controller('<%= moduleName %><%= capitalizedName %>Controller', <%= moduleName %><%= capitalizedName %>Controller);


    /**
     * @memberOf <%= modulePath %>.<%= lowercaseName %>
     * @name <%= capitalizedName %>RouteConfig
     */
    function <%= moduleName %><%= capitalizedName %>RouteConfig($routeProvider) {
        $routeProvider
          .when('/<%= moduleName %>/<%= url %>', {
            navigationKey: '<%= moduleName %><%= capitalizedName %',
            templateUrl:   '<%= templateUrl %>',
            controller:    '<%= moduleName %><%= capitalizedName %>Controller',
            controllerAs:  '<%= moduleName %><%= capitalizedName %'
          });
    }

    /**
     * @memberOf <%= modulePath %>.<%= lowercaseName %>
     * @name <%= capitalizedName %>Controller
     */
    function <%= moduleName %><%= capitalizedName %>Controller(<%= dependencies %>) {
        var vm = this;

        // code goes here...

    }


}());
