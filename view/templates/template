/**
 * <%= capitalizedName %>
 * @namespace <%= moduleName %>
 */
(function() {
    'use strict';

    /**
     * @memberOf <%= moduleName %>
     * @namespace <%= capitalizedName %>
     *
     * @description
     * <%= description %>
     * Route: /<%= url %>
     */
    angular
        .module('<%= moduleName %>.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .config(<%= capitalizedName %>RouteConfig)
        .controller('<%= capitalizedName %>Controller', <%= capitalizedName %>Controller);


    /**
     * @memberOf <%= moduleName %>.<%= capitalizedName %>
     * @name <%= capitalizedName %>RouteConfig
     */
    function <%= capitalizedName %>RouteConfig($routeProvider) {
        $routeProvider
          .when('/<%= url %>', {
            navigationKey: '<%= capitalizedName %>',
            templateUrl:   '<%= templateUrl %>',
            controller:    '<%= prefix %><%= capitalizedName %>Controller',
            controllerAs:  '<%= capitalizedName %>'
          });
    }

    /**
     * @memberOf <%= moduleName %>.<%= capitalizedName %>
     * @name <%= capitalizedName %>Controller
     */
    function <%= capitalizedName %>Controller(<%= dependencies %>) {
        var vm = this;

        // code goes here...

    }


}());
