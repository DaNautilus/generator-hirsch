/**
 * <%= capitalizedName %>
 * @namespace <%= modulePath %>
 */
(function() {
    'use strict';

    /**
     * @memberOf <%= modulePath %>
     * @namespace <%= lowercaseName %>
     *
     * @description
     * <%= description %>
     * Route: /<%= url %>
     */
    angular
        .module('<%= modulePath %>.<%= lowercaseName %>', [
            <%= modules %>
        ])
        .config(<%= capitalizedName %>RouteConfig)
        .controller('<%= modulePath %><%= capitalizedName %>Controller', <%= capitalizedName %>Controller);


    /**
     * @memberOf <%= modulePath %>.<%= lowercaseName %>
     * @name <%= capitalizedName %>RouteConfig
     */
    function <%= capitalizedName %>RouteConfig($routeProvider) {
        $routeProvider
          .when('/<%= modulePath %>/<%= url %>', {
            navigationKey: '<%= modulePath %><%= capitalizedName %>',
            templateUrl:   '<%= templateUrl %>',
            controller:    '<%= modulePath %><%= capitalizedName %>Controller',
            controllerAs:  '<%= modulePath %><%= capitalizedName %>'
          });
    }

    /**
     * @memberOf <%= modulePath %>.<%= lowercaseName %>
     * @name <%= capitalizedName %>Controller
     */
    function <%= capitalizedName %>Controller(<%= dependencies %>) {
        var vm = this;

        // code goes here...

    }


}());
